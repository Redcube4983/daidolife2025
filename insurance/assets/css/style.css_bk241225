            transform: translateY(-7px) rotate(-45deg);
  }
  50% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
  }
  100% {
    -webkit-transform: translateY(-15px) rotate(0deg);
            transform: translateY(-15px) rotate(0deg);
  }
}
@-webkit-keyframes menu-bottom {
